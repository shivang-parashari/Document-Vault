            <a href="~/@Model.ScannedDoc">Download_Document</a>
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Document_Date_Detail)
        </dt>

        <dd class="col-sm-10">
            <a class="btn btn-outline-primary" asp-area="" asp-controller="Document_Date_Detail" asp-action="Index" >Date Detail</a>
        </dd>


    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.DocId">Edit</a> |
    <a asp-action="Index">Back to List</a>
